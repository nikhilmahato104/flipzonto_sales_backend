      async function downloadInvoice(orderId) {
        const { jsPDF } = window.jspdf;

        if (typeof QRCode === "undefined") {
          alert("QRCode library not loaded.");
          return;
        }

        const orderEl = document.getElementById(`order-${orderId}`);
        if (!orderEl) {
          console.error(`Order element with ID 'order-${orderId}' not found.`);
          return;
        }

        const doc = new jsPDF({ unit: "pt", format: "a4" });
        const pageWidth = doc.internal.pageSize.getWidth();
        const pageHeight = doc.internal.pageSize.getHeight();

        const sideMargin = 10;
        const topMargin = 40;
        let y = topMargin;
        const qrSize = 70;
        const cellPadding = 5;
        const defaultFont = "Helvetica";
        const defaultFontSize = 11;
        const lineHeight = 15;

        doc.setFont(defaultFont, "normal");
        doc.setFontSize(defaultFontSize);

        const rows = Array.from(orderEl.querySelectorAll("tbody tr"));
        let totalQty = 0;
        let totalPrice = 0;

        // Calculate totals
        rows.forEach((row) => {
          const cells = row.querySelectorAll("td");
          const rate = parseFloat(
            cells[2]?.textContent.replace(/[₹,]/g, "") || "0"
          );
          const qty = parseFloat(cells[3]?.textContent || "0");
          totalQty += qty;
          totalPrice += rate * qty;
        });

        // Add QR
        const qrContent = `Order ID: ${orderId}\nTotal Items: ${totalQty}\nTotal Price: ₹${totalPrice.toFixed(
          2
        )}`;
        const qrDataUrl = await QRCode.toDataURL(qrContent, {
          margin: 1,
          width: qrSize,
        });
        doc.addImage(qrDataUrl, "PNG", sideMargin, y, qrSize, qrSize);

        // Header
        // doc.setFontSize(18);
        // doc.text('FlipZonto Invoice', sideMargin + qrSize + 20, y + 20);

        const logoWidth = 100; // adjust size as needed
        const logoHeight = 30;

        const logoDataUrl =
          "data:image/png;base64,"; // <-- paste your base64 here

        doc.addImage(
          logoDataUrl,
          "PNG",
          sideMargin + qrSize + 20,
          y,
          logoWidth,
          logoHeight
        );

        doc.setFontSize(12);
        doc.text(
          `Invoice Number: ${orderId}`,
          sideMargin + qrSize + 20,
          y + 40
        );
        doc.setFontSize(10);
        doc.text(
          "Thank you for shopping with FlipZonto",
          sideMargin + qrSize + 20,
          y + 55
        );
        y += qrSize + 40;

        function removeHindi(text) {
          return text.replace(/[\u0900-\u097F]+/g, "").trim();
        }

        function drawLabelValue(label, value) {
          doc.setFontSize(defaultFontSize);
          const cleanValue = removeHindi(value);
          const labelX = sideMargin;
          const valueX = sideMargin + 150;
          const lines = doc.splitTextToSize(
            cleanValue,
            pageWidth - valueX - sideMargin
          );
          doc.text(`${label}:`, labelX, y);
          doc.text(lines, valueX, y);
          y += lines.length * lineHeight;
        }

        const getText = (selector, label) =>
          removeHindi(
            orderEl
              .querySelector(selector)
              ?.textContent.replace(label, "")
              .trim() || ""
          );

        const shopName = removeHindi(
          orderEl
            .querySelector("h2")
            ?.textContent.replace("Shop: ", "")
            .trim() || "Invoice"
        );
        const mapLink = orderEl.querySelector(
          ".shop-info p:nth-of-type(5) a"
        )?.href;

        drawLabelValue("Buyer (Shop) Name", shopName);
        drawLabelValue(
          "Owner",
          getText(".shop-info p:nth-of-type(1)", "Owner:")
        );
        drawLabelValue(
          "Contact",
          getText(".shop-info p:nth-of-type(2)", "Contact:")
        );
        drawLabelValue(
          "Address",
          getText(".shop-info p:nth-of-type(3)", "Address:")
        );
        drawLabelValue(
          "Region",
          getText(".shop-info p:nth-of-type(4)", "Region:")
        );

        if (mapLink) {
          doc.text("Location:", sideMargin, y);
          doc.setTextColor(0, 0, 255);
          doc.textWithLink("Google Maps Link", sideMargin + 150, y, {
            url: mapLink,
          });
          doc.setTextColor(0, 0, 0);
          y += lineHeight;
        }

        drawLabelValue(
          "Salesman Info",
          getText(".shop-info p:nth-of-type(6)", "Salesman Info:")
        );
        drawLabelValue(
          "Comment",
          getText(".shop-info p:nth-of-type(7)", "Comment:")
        );
        drawLabelValue(
          "Order Date",
          getText(".shop-info p:nth-of-type(8)", "Order Date:")
        );
        y += 10;

        // Table header
        const tableX = sideMargin;
        const totalTableWidth = pageWidth - 2 * sideMargin;
        const colWidths = [
          0.07 * totalTableWidth,
          0.38 * totalTableWidth,
          0.17 * totalTableWidth,
          0.11 * totalTableWidth,
          0.09 * totalTableWidth,
          0.18 * totalTableWidth,
        ];
        const headers = [
          "S.No",
          "Item Name",
          "Category",
          "Rate (Rs)",
          "Quantity",
          "Total (Rs)",
        ];

        doc.setFontSize(12);
        doc.setFillColor(220);
        let colX = tableX;
        headers.forEach((header, i) => {
          doc.rect(colX, y, colWidths[i], lineHeight * 2);
          doc.text(header, colX + colWidths[i] / 2, y + 15, {
            align: "center",
          });
          colX += colWidths[i];
        });
        y += lineHeight * 2;

        doc.setFontSize(defaultFontSize);
        let subtotal = 0;

        rows.forEach((row, idx) => {
          const cells = row.querySelectorAll("td");
          const itemName = removeHindi(cells[0]?.textContent || "");
          const category = removeHindi(cells[1]?.textContent || "");
          const rate = parseFloat(
            cells[2]?.textContent.replace(/[₹,]/g, "") || "0"
          );
          const qty = parseFloat(cells[3]?.textContent || "0");
          const total = rate * qty;
          subtotal += total;

          const rowData = [
            `${idx + 1}`,
            itemName,
            category,
            rate.toFixed(2),
            qty.toString(),
            total.toFixed(2),
          ];

          const linesPerCell = rowData.map((text, i) =>
            doc.splitTextToSize(text, colWidths[i] - 2 * cellPadding)
          );
          const maxLines = Math.max(
            ...linesPerCell.map((lines) => lines.length)
          );
          const rowHeight = maxLines * lineHeight;

          let x = tableX;
          linesPerCell.forEach((lines, i) => {
            doc.rect(x, y, colWidths[i], rowHeight);
            lines.forEach((line, j) => {
              doc.text(
                line,
                x + cellPadding,
                y + lineHeight + j * lineHeight - 3
              );
            });
            x += colWidths[i];
          });
          y += rowHeight;

          if (y > pageHeight - 100) {
            doc.addPage();
            y = topMargin;
          }
        });

        // Subtotal section
        y += 10;
        doc.setFontSize(13);
        doc.setFillColor(240);
        doc.rect(sideMargin, y - 5, pageWidth - 2 * sideMargin, 20, "F");
        doc.text(`Subtotal: ${subtotal.toFixed(2)}`, pageWidth / 2, y + 10, {
          align: "center",
        });
        y += 25;

        // Amount in words
        const words = numberToWords(subtotal).replace(/\b\w/g, (l) =>
          l.toUpperCase()
        );
        doc.setFontSize(defaultFontSize);
        doc.setFillColor(240);
        doc.rect(sideMargin, y - 5, pageWidth - 2 * sideMargin, 20, "F");
        doc.text(words, pageWidth / 2, y + 10, { align: "center" });
        y += 30;

        // Outer border
        // Outer border - with 0.5 cm (approx. 14.175 pt) margin
        const marginPt = 5.175;

        doc.setDrawColor(0);
        doc.setLineWidth(1.2);
        doc.rect(
          marginPt, // x
          marginPt, // y
          pageWidth - 2 * marginPt, // width
          pageHeight - 2 * marginPt // height
        );

        const fileName = `${shopName.replace(/\s+/g, "_")}_${orderId}.pdf`;
        doc.save(fileName);
      }
    </script>
  </head>
  <body>
    <button class="refresh-btn-page" onclick="refreshPage()">
      Refresh Page 🔄
    </button>

    <h1>All Order details Orders</h1>

    <div class="filters">
      <label for="filterDate">Order Date:</label>
      <input type="date" id="filterDate" />

      <label for="filterShipped">Shipped:</label>
      <select id="filterShipped">
        <option value="">All</option>
        <option value="true">Shipped</option>
        <option value="false">Not Shipped</option>
      </select>

      <button class="refresh-btn" onclick="applyFilters()">
        Apply Filters 🔍
      </button>
      <button class="refresh-btn" onclick="refreshPage()">
        Reset Filters 🔄
      </button>
    </div>

    <% if (orders.length === 0) { %>
    <p class="no-orders">No orders found.</p>
    <% } else { %> <% orders.slice().reverse().forEach(order => { %>

    <div
      class="order <%= order.shipped ? 'shipped' : '' %>"
      id="order-<%= order._id %>"
      data-date="<%= order.orderDate ? order.orderDate.toISOString().split('T')[0] : '' %>"
      data-shipped="<%= order.shipped %>"
    >
      <div class="order-header">
        <div class="shop-info">
          <h2>Shop: <%= order.shopDetails.shopName %></h2>
          <p>Owner: <%= order.shopDetails.shopOwnerName %></p>
          <p>Contact: <%= order.shopDetails.contact %></p>
          <!-- <p>Address: <%= order.shopDetails.address %>, <%= order.shopDetails.region %> - <%= order.shopDetails.location %></p> -->
          <p>Address: <%= order.shopDetails.address %></p>
          <p>Region: <%= order.shopDetails.region %></p>
          <p>
            Google Maps:
            <a href="<%= order.shopDetails.location %>" target="_blank"
              >View on Maps</a
            >
          </p>

          <p>
            Salesman: <%= order.salesmanName %> (Contact: <%=
            order.salesmanContact %>)
          </p>
          <p>Comment: <%= order.comment %></p>
          <p>
            Order Date: <%= order.orderDate ? order.orderDate.toDateString() :
            '' %>
          </p>
          <p>
            Status:
            <strong
              ><%= order.shipped ? 'Shipped ✅' : 'Not Shipped ❌' %></strong
            >
          </p>
        </div>
        <div>
          <img
            class="shop-image"
            src="<%= order.shopDetails.imageUrl %>"
            alt="Shop Image"
          />
        </div>
      </div>

      <h3>Cart Items:</h3>
      <table>
        <thead>
          <tr>
            <th>Item Name</th>
            <th>Category</th>
            <th>Rate (₹)</th>
            <th>Quantity</th>
            <th>Image</th>
          </tr>
        </thead>
        <tbody>
          <% order.cartItems.forEach(item => { %>
          <tr>
            <td><%= item.name %></td>
            <td><%= item.category %></td>
            <td><%= item.rate %></td>
            <td><%= item.quantity %></td>
            <td><img src="<%= item.image %>" alt="<%= item.name %>" /></td>
          </tr>
          <% }) %>
        </tbody>
      </table>

      <button class="toggle-btn" onclick="toggleShipped('<%= order._id %>')">
        Toggle Shipped
      </button>
      <button class="toggle-btn" onclick="downloadInvoice('<%= order._id %>')">
        Download Invoice 🧾
      </button>
    </div>
    <% }) %> <% } %>

    <script>
      function refreshPage() {
        location.reload();
      }
    </script>
  </body>
